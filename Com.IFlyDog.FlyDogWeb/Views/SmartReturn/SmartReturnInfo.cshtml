                <input type="text" name="date" id="dateEditName" lay-verify="date" placeholder="退货日期" autocomplete="off" class="layui-input" value="{{=it.CreateTime}}" onclick="layui.laydate({ elem: this, min: '1900-01-01 00:00:00' })">
