@using Com.IFlyDog.FlyDogWeb.Helper
<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <title>科室发料</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/Styles/Public/base-1.0.css" rel="stylesheet" />
    <link href="~/lay_component/plugins/layui/css/layui.css" rel="stylesheet" media="all" />
    <link href="~/lay_component/css/global.css" rel="stylesheet" />
</head>
<body>
<div class="admin-main"> 
    <fieldset class="layui-elem-field">
        <legend>科室发料</legend>
        <div class="layui-field-box">
            <div class="layui-tab layui-tab-brief" lay-filter="docDemoTabBrief">
                <ul class="layui-tab-title info">
                    <li class="layui-this DeptSend" >发货请求</li>
                    <li class="DeptSendToday" >今日发货记录</li> 
                </ul>
                <div class="layui-tab-content">
                    <div class="layui-tab-item layui-show">
                        <table class="site-table table-hover">
                            <thead>
                                <tr>
                                    <th>顾客编号</th>
                                    <th>顾客姓名</th>
                                    <th>划扣项目</th>
                                    <th>划扣时间</th>
                                    <th>发货仓库</th> 
                                    <th>耗材名称</th>
                                    <th>规格</th>
                                    <th>单位</th>
                                    <th>数量</th>
                                    <th>操作</th>
                                </tr>
                            </thead>
                            <tbody class="tb-DeptSend">
                               
                            </tbody>
                        </table>
                    </div>
                    <div class="layui-tab-item">
                        <table class="site-table table-hover">
                            <thead>
                                <tr>
                                    <th>顾客编号</th>
                                    <th>顾客姓名</th> 
                                    <th>划扣时间</th>
                                    <th>发货仓库</th>
                                    <th>耗材名称</th>
                                    <th>规格</th>
                                    <th>单位</th>
                                    <th>数量</th> 
                                    <th>操作人</th> 
                                </tr>
                            </thead>
                            <tbody class="tb-DeptSendToday">
                             
                            </tbody>
                        </table>
                    </div>
               
                </div>
            </div> 
        </div>
    </fieldset>

</div>
 
<div class="dot-tmp">
    <!--待发货-->
    <script type="text/x-dot-template" class="DeptSend-temp">
        {{ for(var item in it){ }}
             <tr  send-Id="{{= it[item].ID }}">
                 <td>{{= it[item].CustomerID }}</td> 
                 <td><a class="pointer addCustomerTab">{{= it[item].CustomerName}}</a></td>
                 <td>{{= it[item].ChargeName }}</td> 
                 <td>{{= it[item].CreateTime == null ? "暂无": it[item].CreateTime.slice(0,10)}}</td> 
                 <td>{{= it[item].Warehouse }}</td> 
                 <td>{{= it[item].Product }}</td> 
                 <td>{{= it[item].Size }}</td> 
                 <td>{{= it[item].UnitName }}</td> 
                 <td>{{= it[item].Num }}</td> 
                 <td><span class="layui-btn layui-btn-small layui-btn-normal sen-btn">发货</span></td>
            </tr>
        {{ } }}
    </script>
    <!--今日发货记录-->
    <script type="text/x-dot-template"  class="DeptSendToday-temp">
        {{ for(var item in it){ }}
        <tr>
            <td>{{= it[item].CustomerID }}</td>
            <td><a class="pointer addCustomerTab">{{= it[item].CustomerName}}</a></td> 
            <td>{{= it[item].CreateTime == null ? "暂无": it[item].CreateTime.slice(0,10)}}</td>
            <td>{{= it[item].Warehouse }}</td>
            <td>{{= it[item].Product }}</td>
            <td>{{= it[item].Size }}</td>
            <td>{{= it[item].UnitName }}</td>
            <td>{{= it[item].Num }}</td>
            <td>{{= it[item].CreateUserName }}</td>
        </tr>
        {{ } }}
    </script>
</div>
<script src="~/lay_component/js/jquery-2.0.3.min.js"></script>
<script src="~/lay_component/js/hcommon.js"></script>
<script src="~/Scripts/FlyDog.Common.js"></script>
<script src="~/doT-master/doT-master/doT.min.js"></script>
<script src="~/lay_component/plugins/layui/layui.js"></script>
<script src="~/lay_component/sampleJs/DeptSend.js"></script>
</body>
</html>
